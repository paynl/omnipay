{
    "name": "paynl/omnipay-paynl-v3",
    "description": "Pay.nl driver for the Omnipay PHP payment processing library based on the V3 PayNL orders API",
    "type": "library",
    "require": {
        "php": "^7.4|^8",
        "phpunit/phpunit": "^10.0",
        "omnipay/common": "^3.3",
        "ext-json": "*"
    },
    "require-dev": {
        "php-http/guzzle7-adapter": "^1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omnipay\\PaynlV3\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
